package com.sohu.sur.base.exception;

/**
 * Sis远程调用异常
 * 
 * @author xiayanming
 *
 */
public class SucRemoteException extends SucException {

	/**
     * Class constructor.
     */
    public SucRemoteException() {
        super();
    }

    /**
     * Class constructor.
     *
     * @param message	java.lang.String
     */
    public SucRemoteException(String message) {
        super(message);
    }

    /**
     * Class constructor.
     *
     * @param message
     * @param cause
     */
    public SucRemoteException(String message, Throwable cause) {
        super(message, cause);
    }

    /**
     * Class constructor.
     *
     * @param cause
     */
    public SucRemoteException(Throwable cause) {
        super(cause);
    }
}
