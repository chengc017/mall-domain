package com.sohu.sur.util.gift;

/**
 * 下发短信，邮件 结果
 * @author Administrator
 *
 */
public class ResultBean {

	public static final ResultBean OK = new ResultBean(0, "OK");

	public static final ResultBean FAIL = new ResultBean(-1, "FAIL");
	
    private int status; 
    private String msg;
    private String refresh;
    
    
    
	public ResultBean() {
		super();
	}


	public ResultBean(int status, String msg) {
		super();
		this.status = status;
		this.msg = msg;
	}


	public ResultBean(int status, String msg, String refresh) {
		super();
		this.status = status;
		this.msg = msg;
		this.refresh = refresh;
	}


	/**
	 * @return the result
	 */
	public int getStatus() {
		return status;
	}


	/**
	 * @param status the status to set
	 */
	public void setStatus(int status) {
		this.status = status;
	}


	/**
	 * @return the msg
	 */
	public String getMsg() {
		return msg;
	}


	/**
	 * @param msg the msg to set
	 */
	public void setMsg(String msg) {
		this.msg = msg;
	}


	/**
	 * @return the refresh
	 */
	public String getRefresh() {
		return refresh;
	}


	/**
	 * @param refresh the refresh to set
	 */
	public void setRefresh(String refresh) {
		this.refresh = refresh;
	}
    

	
	
}
